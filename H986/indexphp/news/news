		<script type="text/javascript">
			(function () {
				/*组件初始化js begin*/
				var count = 0;     //demo演示使用变量
				$('.ui-refresh1').refresh({
					ready: function (dir, type) {
						var me = this;
						$.getJSON('news/refresh/1/10/'+count, function (data) {
							var $list = $('.data-list1'),
									html = (function (data) {      //数据渲染
										var liArr = [];
										$.each(data, function () {
											liArr.push(this.html);
										});
										return liArr.join('');
									})(data);

							$list[dir == 'up' ? 'prepend' : 'append'](html);
							var abox = $('#tabs1 .ui-tabs-content')
							var aheight = abox.height()
							for (var i = 0;i < 10;i++) {
								abox.css('height', aheight + 360);
							}
							me.afterDataLoading();    //数据加载完成后改变状态
							++count > 19 && me.disable(dir, true);    //加载5次后，按钮隐藏
						});
					}
				});
				/*组件初始化js end*/
			})();
		</script>
