function switchOrientation(){
	var orientation = window.orientation;
	if(orientation == 0){
		$("body").addClass("portrait");
		$("body").removeClass("landscape");

	}else{
		$("body").addClass("landscape");
		$("body").removeClass("portrait");
	}
	
	video_padding_top();
	video_sp_bg();	
	flexs();
	carousel_li_height();
	fGetScript();

}

switchOrientation();
