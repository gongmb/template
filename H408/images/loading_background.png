function carousel_li_height(){

var lis = $('.flexslider .slides li').not('#works .flexslider .slides li,#video .flexslider .slides li');

if($('html').width() < 640){
	var hw = $('html').width()*0.85;
	lis.height(hw+"px");
	lis.width(hw+"px");
	var hfs = hw*0.563;
	$('#video .flexslider .slides li .no').css({"height": hfs+"px"});	
}else{
	lis.css({"height":"auto","width":"auto"});
}

}

function video_padding_top(){

if($('html').width() > 640){
	var vww = ($('#video_bg').height()-794)/2;
	$('#video_wrap').css("padding-top",vww+"px");
	return true;
}else{
	$('#video_wrap').css("padding-top","0");
}

}

function video_sp_bg(){

if($('html').width() < 640){
	var h = $('#video_sp_unit li').height();	
	var w = $('#video_sp_unit li').width();
	h = h+(h*0.2);
	var padding_top = (h*0.08);
	w = w+(w*0.1);
	var l = ($('html').width()-w)/2;
	$('#video_sp_unit #sls_bg').css({"width":w+"px","height":h+"px","left":l+"px"});
	$('#video_sp_unit .sls-view').css({"height":h+"px","margin-top":padding_top+"px"});
}

}

$("section#video #playerbox #player").css("z-index","100");
video_padding_top();
video_sp_bg();
carousel_li_height();

